        '                  Select '#39'ESTATAL'#39'    Tipo_Entidad, '#39'Producto4'#39' ' +
        'Producto, 2 Orden,   9  B_Registro From Dual Union'
      
        '                  Select '#39'ESTATAL'#39'    Tipo_Entidad, '#39'Producto5'#39' ' +
        'Producto, 2 Orden,   10 B_Registro From Dual Union'
      
        '                  Select '#39'ESTATAL'#39'    Tipo_Entidad, '#39'Producto6'#39' ' +
        'Producto, 2 Orden,   11 B_Registro From Dual Union'
      
        '                  Select '#39'ESTATAL'#39'    Tipo_Entidad, '#39'Producto7'#39' ' +
        'Producto, 2 Orden,   12 B_Registro From Dual Union'
      
        '                  Select '#39'ESTATAL'#39'    Tipo_Entidad, '#39'Producto8'#39' ' +
        'Producto, 2 Orden,   13 B_Registro From Dual Union'
      
        '                  Select '#39'ESTATAL'#39'    Tipo_Entidad, '#39'Producto9'#39' ' +
        'Producto, 2 Orden,   14 B_Registro From Dual Union'
      
        '                  Select '#39'Total'#39'      Tipo_Entidad,  Null       ' +
        'Producto, 2 Orden,   15 B_Registro From Dual Union'
      
        '                  Select '#39'MUNICIPAL'#39'  Tipo_Entidad, '#39'Producto1'#39' ' +
        'Producto, 3 Orden,   17 B_Registro From Dual Union  '
      
        '                  Select '#39'MUNICIPAL'#39'  Tipo_Entidad, '#39'Producto2'#39' ' +
        'Producto, 3 Orden,   18 B_Registro From Dual Union'
      
        '                  Select '#39'MUNICIPAL'#39'  Tipo_Entidad, '#39'Producto3'#39' ' +
        'Producto, 3 Orden,   19 B_Registro From Dual Union'
      
