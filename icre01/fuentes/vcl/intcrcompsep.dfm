  object QDetalles: TQuery
    SQL.Strings = (
      'Select T.Titulos,'
      '       S.Tipo_Entidad, '
      '       S.Nom_Acred_Reg,'
      '       S.Id_Acreditado,'
      '       S.Id_Contrato,'
      '       S.Id_Credito,'
      '       S.Tipo_Credito,'
      '       S.Orden, '
      '       S.Producto, '
      '       P.B_Registro, '
      
        '       Round (Saldo_T4 / :peFactor,     Case When :peFactor = 10' +
        '00000 Then 0 Else 2 End) Saldo_T4Ant, '
      
        '       Round (Saldo_Actual / :peFactor, Case When :peFactor = 10' +
        '00000 Then 0 Else 2 End) Saldo_Actual'
      '  From ( '
      '        Select R.Tipo_Entidad Tipo_Entidad,'
      '               R.Nom_Acred_Reg,'
      '               R.Id_Acreditado,'
      '               R.Id_Contrato,'
      '               R.Id_Credito,'
      '               R.Tipo_Credito,'
      '               O.Orden Orden,'
      '               R.Producto, '
      
        '               Nvl (Saldo_T4,0)                        Saldo_T4,' +
        ' '
      
        '               Nvl (Saldo_Actual,0)                    Saldo_Act' +
        'ual '
      '          From ( '
      '                Select C.Tipo_Entidad, '
      '                       C.Nom_Acred_Reg,'
      '                       C.Id_Acreditado,'
      '                       C.Id_Contrato,'
      '                       C.Id_Credito,'
      '                       C.Tipo_Credito,'
      
        '                       Case When C.Id_Acreditado =  439300 And C' +
        '.Tipo_Entidad = '#39'FEDERAL'#39' Then '#39'Cartera'#39' '
      
        '                            When C.Id_Acreditado <> 439300 And C' +
        '.Tipo_Entidad = '#39'FEDERAL'#39' Then '#39'Factoraje'#39' Else C.Producto End P' +
        'roducto, '
      '                       0 Saldo_T4, '
      
        '                       (Nvl (C.Imp_Cap_Vig,     0) + Nvl (C.Imp_' +
        'Cap_Imp,    0) +'
      
        '                        Nvl (C.Imp_Cap_Vdo_NE,  0) + Nvl (C.Imp_' +
        'Cap_Vdo_Ex, 0) +'
      
        '                        Nvl (C.Imp_Int_Vig,     0) + Nvl (C.Imp_' +
        'Int_Imp,    0) +'
      
        '                        Nvl (C.Imp_Int_Vdo_NE,  0) + Nvl (C.Imp_' +
        'Int_Vdo_Ex, 0) -'
      
        '                        Case :peInt_Cob_Ant When '#39'V'#39' Then Nvl (C' +
        '.Imp_Int_Cob_Ant, 0) Else 0 End'
      '                       ) *'
      
        '                               Pkgcrconsolidado1.Fnc_Obt_Tipo_Ca' +
        'mbio_Regul '
      
        '                               (C.Cve_Moneda, '#39'D000'#39', Extract (Y' +
        'ear From :peFecha),Extract (Month From :peFecha)) Saldo_Actual '
      
        '                 From  Table (Pkgcrgpviews.VW_CR_GP_Consolidado_' +
        'Edos_Mun(:peFecha, '#39'FEDERAL|ESTATAL|MUNICIPAL|ORG-DESC|'#39')) C'
      
        '                Where (:peCartas_Cred = '#39'V'#39' Or Tipo_Credito   = ' +
        #39'CRED'#39')'
      '                  And C.Producto <> '#39'No Aplica'#39' '
      '                 Union All'
      '                Select C.Tipo_Entidad, '
      '                       C.Nom_Acred_Reg,'
      '                       C.Id_Acreditado,'
      '                       C.Id_Contrato,'
      '                       C.Id_Credito,'
      '                       C.Tipo_Credito,'
      
        '                       Case When C.Id_Acreditado =  439300 And C' +
        '.Tipo_Entidad = '#39'FEDERAL'#39' Then '#39'Cartera'#39' '
      
        '                            When C.Id_Acreditado <> 439300 And C' +
        '.Tipo_Entidad = '#39'FEDERAL'#39' Then '#39'Factoraje'#39' Else C.Producto End P' +
        'roducto, '
      
        '                       (Nvl (C.Imp_Cap_Vig,     0) + Nvl (C.Imp_' +
        'Cap_Imp,    0) +'
      
        '                        Nvl (C.Imp_Cap_Vdo_NE,  0) + Nvl (C.Imp_' +
        'Cap_Vdo_Ex, 0) +'
      
        '                        Nvl (C.Imp_Int_Vig,     0) + Nvl (C.Imp_' +
        'Int_Imp,    0) +'
      
        '                        Nvl (C.Imp_Int_Vdo_NE,  0) + Nvl (C.Imp_' +
        'Int_Vdo_Ex, 0) -'
      
        '                        Case :peInt_Cob_Ant When '#39'V'#39' Then Nvl (C' +
        '.Imp_Int_Cob_Ant, 0) Else 0 End'
      '                       ) *'
      
        '                               Pkgcrconsolidado1.Fnc_Obt_Tipo_Ca' +
        'mbio_Regul '
      
        '                               (C.Cve_Moneda, '#39'D000'#39', Extract (Y' +
        'ear From :peFecha),Extract (Month From :peFecha)) Saldo_T4, '
      '                       0 Saldo_Actual '
      
        '                 From  Table (Pkgcrgpviews.VW_CR_GP_Consolidado_' +
        'Edos_Mun(Trunc(:peFecha, '#39'yyyy'#39') - 1 , '#39'FEDERAL|ESTATAL|MUNICIPA' +
        'L|ORG-DESC|'#39')) C'
      
