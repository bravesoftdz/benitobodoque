  object QDatos: TQuery
    SQL.Strings = (
      'Select Titulos,'
      '       Orden_Estructura,'
      '       B_Registro,'
      '       Case When B_Titulo = 1  Then Null '
      '            Else Nvl (Round ( I.Saldo_T4  /:peFactor, '
      
        '                              Case When :peFactor = 1000000 Then' +
        ' 0 Else 2 End), 0)  '
      
        '                                                                ' +
        'End Imp_T4YAnt,     '
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else Nvl (I.Pct_Saldo_T4, 0)                        ' +
        'End Pct_T4YAnt,'
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else 0                                              ' +
        'End Imp_T1YAct, '
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else 0                                              ' +
        'End Pct_T1YAct,'
      '       Case When B_Titulo = 1  Then Null '
      '            Else Nvl (Round (I.Saldo_Actual /:peFactor,'
      
        '                             Case When :peFactor = 1000000 Then ' +
        '0 Else 2 End), 0)'
      
        '                                                                ' +
        'End Imp_Saldo_al, '
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else Nvl (I.Pct_Saldo_Actual, 0)                    ' +
        'End Pct_Saldo_al,'
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else 0                                              ' +
        'End Imp_T1_Diferencia,    '
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else 0                                              ' +
        'End Pct_T1_Diferencia, '
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else 0                                              ' +
        'End Imp_T4Yact,     '
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else 0                                              ' +
        'End Pct_T4YAct,'
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else 0                                              ' +
        'End Imp_T4_Diferencia,    '
      '       Case When B_Titulo = 1  Then Null '
      
        '            Else 0                                              ' +
        'End Pct_T4_Diferencia       '
      '  From ( '
      '        Select Titulos, '
      '               RowNum Orden_Estructura,'
      
        '               Case When RowNum In (1,5,16,27) Then 1 Else 0 End' +
        ' B_Titulo '
      '         From( '
      
        '              Select '#39'Gobierno Federal'#39'            Titulos, 1  O' +
        'rden From Dual Union '
      
        '              Select '#39'Cartera'#39'                     Titulos, 2  O' +
        'rden From Dual Union  '
      
        '              Select '#39'Factoraje'#39'                   Titulos, 3  O' +
        'rden From Dual Union '
      
        '              Select '#39'Total'#39'                       Titulos, 4  O' +
        'rden From Dual Union '
      
        '              Select '#39'Gobiernos Estatales'#39'         Titulos, 5  O' +
        'rden From Dual Union '
      
        '              Select '#39'Fid/R28 (1)'#39'                 Titulos, 6  O' +
        'rden From Dual Union '
      
        '              Select '#39'Fid/R33 (2)'#39'                 Titulos, 7  O' +
        'rden From Dual Union '
      
        '              Select '#39'Fid/Rp  (3)'#39'                 Titulos, 8  O' +
        'rden From Dual Union '
      
        '              Select '#39'Man/R28 (4)'#39'                 Titulos, 9  O' +
        'rden From Dual Union '
      
        '              Select '#39'Man/R33 (5)'#39'                 Titulos, 10 O' +
        'rden From Dual Union '
      
        '              Select '#39'Man/Rp  (6)'#39'                 Titulos, 11 O' +
        'rden From Dual Union '
      
        '              Select '#39'Sin Estructura/IF (7)'#39'       Titulos, 12 O' +
        'rden From Dual Union '
      
        '              Select '#39'Sin Estructura/IE (8)'#39'       Titulos, 13 O' +
        'rden From Dual Union '
      
        '              Select '#39'Sin Estructura/SI (9)'#39'       Titulos, 14 O' +
        'rden From Dual Union '
      
        '              Select '#39'Total'#39'                       Titulos, 15 O' +
        'rden From Dual Union '
      
        '              Select '#39'Gobiernos Municipales'#39'       Titulos, 16 O' +
        'rden From Dual Union '
      
        '              Select '#39'Fid/R28 (1)'#39'                 Titulos, 17 O' +
        'rden From Dual Union '
      
        '              Select '#39'Fid/R33 (2)'#39'                 Titulos, 18 O' +
        'rden From Dual Union '
      
        '              Select '#39'Fid/Rp  (3)'#39'                 Titulos, 19 O' +
        'rden From Dual Union '
      
        '              Select '#39'Man/R28 (4)'#39'                 Titulos, 20 O' +
        'rden From Dual Union '
      
        '              Select '#39'Man/R33 (5)'#39'                 Titulos, 21 O' +
        'rden From Dual Union '
      
        '              Select '#39'Man/Rp  (6)'#39'                 Titulos, 22 O' +
        'rden From Dual Union '
      
        '              Select '#39'Sin Estructura/IF (7)'#39'       Titulos, 23 O' +
        'rden From Dual Union '
      
        '              Select '#39'Sin Estructura/IE (8)'#39'       Titulos, 24 O' +
        'rden From Dual Union '
      
        '              Select '#39'Sin Estructura/SI (9)'#39'       Titulos, 25 O' +
        'rden From Dual Union '
      
        '              Select '#39'Total'#39'                       Titulos, 26 O' +
        'rden From Dual Union '
      
        '              Select '#39'Organismos Descentralizados'#39' Titulos, 27 O' +
        'rden From Dual Union '
      
        '              Select '#39'Federal'#39'                     Titulos, 28 O' +
        'rden From Dual Union '
      
        '              Select '#39'Estatal/Municipal'#39'           Titulos, 29 O' +
        'rden From Dual Union '
      
        '              Select '#39'Total'#39'                       Titulos, 30 O' +
        'rden From Dual Union '
      
        '              Select '#39'Gran Total'#39'                  Titulos, 31 O' +
        'rden From Dual  '
      '              Order By Orden'
      '             )'
      '         ) E '
      '   Left Outer  Join ( '
      '                     Select S.Tipo_Entidad, '
      '                            S.Orden, '
      '                            S.Producto, '
      '                            P.B_Registro, '
      '                            Saldo_T4, '
      
        '                            Case When Sum(Saldo_T4) Over (Partit' +
        'ion By S.Tipo_Entidad, S.Orden) = 0 Then 0 '
      
        '                                 Else Saldo_T4 /  Sum(Saldo_T4) ' +
        'Over (Partition By S.Tipo_Entidad, S.Orden) * 100 End Pct_Saldo_' +
        'T4, '
      '                            Saldo_Actual, '
      
        '                            Case When Sum(Saldo_Actual) Over (Pa' +
        'rtition By S.Tipo_Entidad, S.Orden) = 0 Then 0 '
      
        '                                 Else Saldo_Actual /  Sum(Saldo_' +
        'Actual) Over (Partition By S.Tipo_Entidad, S.Orden) * 100 End Pc' +
        't_Saldo_Actual '
      '                      From ( '
      
        '                            Select Case When Grouping (R.Tipo_En' +
        'tidad) = 1 Then '#39'Gran Total'#39' '
      
        '                                        When Grouping (O.Orden) ' +
        '       = 1 Then '#39'Total'#39' Else R.Tipo_Entidad End Tipo_Entidad, '
      '                                   Max(O.Orden) Orden,'
      '                                   R.Producto, '
      '                                   Grouping (R.Tipo_Entidad), '
      '                                   Grouping (O.Orden), '
      
        '                                   Nvl (Sum (Saldo_T4),0)       ' +
        '                 Saldo_T4, '
      
        '                                   Nvl (Sum (Saldo_Actual),0)   ' +
