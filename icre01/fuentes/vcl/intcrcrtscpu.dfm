  object QSecPublico: TQuery
    SQL.Strings = (
      'Select Sector, '
      
        '       Case When G1      = 1 Then Trim ('#39'Total '#39' || Desc_Sector)' +
        ' '
      '            When Renglon = 1 Then Desc_Sector'
      '            Else Null'
      
        '       End                                                      ' +
        '      Desc_Sector,'
      '       Case When G1 = 0 And Renglon = 1 Then Pct_Max_Sector'
      '            Else Null'
      
        '       End                                                      ' +
        '      Pct_Max_Sector,'
      '       Subsector,'
      '       Desc_Subsector,'
      
        '       Pct_Max_Subsector                                        ' +
        '      Pct_Maximo,'
      '       Imp_Real, '
      '       Pct_Real,'
      '       Case When Pct_Real < Pct_Max_Subsector Then '#39'SI'#39' '
      '            Else '#39'NO'#39' '
      
        '       End                                                      ' +
        '      Cumplimiento,'
      '       G1'
      '  From (       '
      '        Select Sector,'
      
        '               Row_Number () Over (Partition By Sector Order By ' +
        'Subsector, G1)                             Renglon,'
      '               Desc_Sector,'
      '               Pct_Max_Sector,'
      '               Subsector,'
      '               Desc_Subsector, '
      '               Pct_Max_Subsector,'
      
        '               Round (Nvl (Imp_Real, 0) / :peFactor, Case When :' +
        'peFactor = 1000000 Then 0 Else 2 End)      Imp_Real,'
      
        '               Round (Case When Sum (Imp_Real) Over (Partition B' +
        'y Sector, G1) = 0 Then 0 '
      
        '                           Else Imp_Real / Sum (Imp_Real) Over (' +
        'Partition By Sector, G1)'
      
        '                      End * 100, 2)                             ' +
        '                                           Pct_Real,'
      '               G1'
      '          From ( '
      '                Select Sec.Sector, '
      '                       Sec.Desc_Sector, '
      '                       Sec.Pct_Max_Sector,'
      '                       Sec.Subsector,'
      '                       Sec.Desc_Subsector,'
      '                       Sec.Pct_Max_Subsector,'
      
        '                       Case When Grouping (Sec.Sector)          ' +
        '  +'
      
        '                                 Grouping (Sec.Desc_Sector)     ' +
        '  + '
      
        '                                 Grouping (Sec.Pct_Max_Sector)  ' +
        '  +'
      
        '                                 Grouping (Sec.Subsector)       ' +
        '  +'
      
        '                                 Grouping (Sec.Desc_Subsector)  ' +
        '  +'
      
        '                                 Grouping (Sec.Pct_Max_Subsector' +
        ') > 0 Then 1'
      '                            Else 0'
      
        '                       End                                      ' +
        '                                                                ' +
        '          G1,'
      '                       Sum ((Nvl (Sdo.Imp_Cap_Vig,     0) + '
      '                             Nvl (Sdo.Imp_Cap_Imp,     0) +'
      '                             Nvl (Sdo.Imp_Cap_Vdo_NE,  0) +'
      '                             Nvl (Sdo.Imp_Cap_Vdo_Ex,  0) +  '
      '                             Nvl (Sdo.Imp_Int_Vig,     0) + '
      '                             Nvl (Sdo.Imp_Int_Imp,     0) +'
      '                             Nvl (Sdo.Imp_Int_Vdo_NE,  0) +'
      '                             Nvl (Sdo.Imp_Int_Vdo_Ex,  0) - '
      
        '                             Case :peInt_Cob_Ant When '#39'V'#39' Then N' +
        'vl (Imp_Int_Cob_Ant, 0) Else 0 End'
      '                            ) *'
      
        '                             PkgCrConsolidado1.Fnc_Obt_Tipo_Camb' +
        'io_Regul (Cve_Moneda, '
      
        '                                                                ' +
        '    '#39'D000'#39', '
      
        '                                                                ' +
        '    To_Char (:peFecha, '#39'YYYY'#39'), '
      
        '                                                                ' +
        '    To_Char (:peFecha, '#39'MM'#39')'
      
        '                                                                ' +
        '   ))                                                           ' +
        '           Imp_Real'
      
        '                  From Table (PkgCRGPViews.VW_CR_GP_Saldos_Edos_' +
        'Mun (:peFecha)) Sdo'
      '                  Full Outer Join ('
      
        '                                   Select Dat.*, Sec.Desc_Sector' +
        '_Eco'
      '                                     From ('
      
        '                                           Select  1 Cve_Sector_' +
        'Eco, 1 Sector, '#39'Primario'#39'   Desc_Sector,   7.5 Pct_Max_Sector, 1' +
        ' SubSector, '#39'Agricultura, ganadería, silvicultura y pesca'#39'      ' +
        '   Desc_Subsector,  7.5 Pct_Max_Subsector From Dual Union'
      
        '                                           Select  2 Cve_Sector_' +
        'Eco, 1 Sector, '#39'Primario'#39'   Desc_Sector,   7.5 Pct_Max_Sector, 1' +
        ' SubSector, '#39'Agricultura, ganadería, silvicultura y pesca'#39'      ' +
        '   Desc_Subsector,  7.5 Pct_Max_Subsector From Dual Union'
      
        '                                           Select  3 Cve_Sector_' +
        'Eco, 1 Sector, '#39'Primario'#39'   Desc_Sector,   7.5 Pct_Max_Sector, 1' +
        ' SubSector, '#39'Agricultura, ganadería, silvicultura y pesca'#39'      ' +
        '   Desc_Subsector,  7.5 Pct_Max_Subsector From Dual Union'
      
        '                                           Select  4 Cve_Sector_' +
        'Eco, 1 Sector, '#39'Primario'#39'   Desc_Sector,   7.5 Pct_Max_Sector, 1' +
        ' SubSector, '#39'Agricultura, ganadería, silvicultura y pesca'#39'      ' +
        '   Desc_Subsector,  7.5 Pct_Max_Subsector From Dual Union'
      
        '                                           Select 60 Cve_Sector_' +
        'Eco, 2 Sector, '#39'Secundario'#39' Desc_Sector,  50.0 Pct_Max_Sector, 1' +
        ' SubSector, '#39'Construcción'#39'                                      ' +
